 let p:Posicao = new Posicao(0,0);
        //Baixo
       if(this.posicao?.linha!<posicao.linha){

             p = new Posicao(this.posicao?.linha!+1,this.posicao?.coluna!);
        
            while(p.linha<posicao.linha){
                if(this.tabuleiro.Mostrapeca(undefined,undefined,p)!==null) return false;
               
                p.linha += Number(1);
            }
            p = new Posicao(0,0);
         return true;
       }
       //Cima
       if(this.posicao?.linha!>posicao.linha){
         p = new Posicao(this.posicao?.linha!-1,this.posicao?.coluna!);
    
        while(p.linha>posicao.linha){
            if(this.tabuleiro.Mostrapeca(undefined,undefined,p)!==null) return false;
            p.linha-=Number(1);
        }
        p = new Posicao(0,0); 
     return true;
   }
 //Direita
   if(this.posicao?.coluna!<posicao.coluna){
     p = new Posicao(this.posicao?.linha!,this.posicao?.coluna!+1);

    while(p.coluna<posicao.coluna){
        if(this.tabuleiro.Mostrapeca(undefined,undefined,p)!==null) return false;
        p.coluna+=Number(1);
    }
    p = new Posicao(0,0);
 return true;
}
//Esquerda
if(this.posicao?.coluna!>posicao.coluna){
     p = new Posicao(this.posicao?.linha!,this.posicao?.coluna!-1);

    while(p.coluna>posicao.coluna){
        if(this.tabuleiro.Mostrapeca(undefined,undefined,p)!==null) return false;
        p.coluna-=Number(1);
    }
    p = new Posicao(0,0);
 return true;